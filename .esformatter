{
    "indent": {
        "ReturnStatement": ">=1",
        "value": "    "
    },
    "jsx": {
        "alignWithFirstAttribute": false,
        "attrsOnSameLineAsTag": false,
        "firstAttributeOnSameLine": true,
        "formatJSX": true,
        "htmlOptions": {
            "indent_size": 4
        },
        "maxAttrsOnTag": 3,
        "spaceInJSXExpressionContainers": ""
    },
    "plugins": [
        "esformatter-jsx",
        "esformatter-quotes",
        "esformatter-eol-last"
    ],
    "preset": "default",
    "quotes": {
        "avoidEscape": true,
        "type": "double"
    },
    "whiteSpace": {
        "before": {
          "ObjectPatternClosingBrace": 1
        },
        "after": {
          "ObjectPatternOpeningBrace": 1
        }
    }
}
